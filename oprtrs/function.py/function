# global & local example

def print_sum_of_numbers(a, b):
    sum = a + b
    print("Hii shivam!, here is the sum of a & b", sum)


print_sum_of_numbers(22, 5)

print("starting")
print("running")

print_sum_of_numbers(99, 1)

my_varable = 10
print(my_varable)
